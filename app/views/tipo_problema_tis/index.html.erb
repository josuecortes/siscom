<p id="notice"><%= notice %></p>

<h1>Tipo Problema Tis</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_problema_tis.each do |tipo_problema_ti| %>
      <tr>
        <td><%= tipo_problema_ti.nome %></td>
        <td><%= tipo_problema_ti.descricao %></td>
        <td><%= link_to 'Show', tipo_problema_ti %></td>
        <td><%= link_to 'Edit', edit_tipo_problema_ti_path(tipo_problema_ti) %></td>
        <td><%= link_to 'Destroy', tipo_problema_ti, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Problema Ti', new_tipo_problema_ti_path %>
