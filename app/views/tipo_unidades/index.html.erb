<p id="notice"><%= notice %></p>

<h1>Tipo Unidades</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_unidades.each do |tipo_unidade| %>
      <tr>
        <td><%= tipo_unidade.nome %></td>
        <td><%= link_to 'Show', tipo_unidade %></td>
        <td><%= link_to 'Edit', edit_tipo_unidade_path(tipo_unidade) %></td>
        <td><%= link_to 'Destroy', tipo_unidade, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Unidade', new_tipo_unidade_path %>
